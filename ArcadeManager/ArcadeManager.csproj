<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ElectronNET.API" Version="11.5.1" />
    <PackageReference Include="CsvHelper" Version="26.0.1" />
  </ItemGroup>
  <ItemGroup>
    <Content Remove="wwwroot\favicon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Views\Help\" />
    <Folder Include="Views\Overlays\" />
    <Folder Include="Views\Roms\" />
    <Folder Include="Views\Csv\" />
    <Folder Include="Behavior\" />
    <Folder Include="Services\" />
    <Folder Include="Actions\" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Data\bios.txt" />
    <None Remove=".DS_Store" />
  </ItemGroup>
  <ItemGroup>
    <None Update="Data\bios.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Content Include="Data\bios.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>
